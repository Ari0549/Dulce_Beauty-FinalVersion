Button} disabled = {index === 1} style={{marginTop: '40px',}}>Previous</Button>\n                    <Button onClick={nextButton} style={{marginTop: '40px',}}>{index === totalPagesCount? '' : 'Next'}</Button>\n                </Row>\n            </Container>\n        </div>\n    )\n\n}\nexport default Book;\n\n","/Users/balerinabrc/Desktop/Dulce_Beauty/src/components/Footer.js",[],[],"/Users/balerinabrc/Desktop/Dulce_Beauty/src/components/Navbar.js",["180"],[],"import Container from 'react-bootstrap/Container';\nimport Nav from 'react-bootstrap/Nav';\nimport Navbar from 'react-bootstrap/Navbar';\nimport Button from 'react-bootstrap/Button';\n\nimport {\n  BrowserRouter as Router,\n  Switch,\n  Route,\n  Link\n} from \"react-router-dom\";\nimport About from \"./About\";\nimport Services from \"./Services\";\nimport Home from \"./Home\";\nimport Book from \"./Book\";\nimport myLogo from './logo.png';\nimport './styles.css';\n\nfunction NavBar(props) {\n  return (\n    <Router>\n      <>\n        <Navbar className='navbar'>\n          <Container>\n            <Nav className=\"links\">\n              <Nav.Link as={Link} to={\"/about\"}>ABOUT</Nav.Link>\n              <Nav.Link as={Link} to={\"/services\"}>SERVICES</Nav.Link>\n            </Nav>\n            <Nav className='brandLogo'>\n              <Navbar.Brand as={Link} to={\"/home\"}>\n                <img src={myLogo}/>\n              </Navbar.Brand>\n            </Nav>\n            <Nav className='links2'>\n              <Nav.Link href=\"#lang\">FR</Nav.Link>\n              <Nav className='buttonBook'>\n                <Nav.Link as={Link} to={\"/book\"}>\n                  <Button bsStyle=\"primary\">Book now</Button>\n                </Nav.Link>\n              </Nav>\n            </Nav>\n          </Container>\n        </Navbar>\n      </>\n      <div>\n        <Switch>\n          <Route path=\"/about\">\n            <About/>\n          </Route>\n          <Route path=\"/services\">\n            <Services />\n   